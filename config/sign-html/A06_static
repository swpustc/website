config/sign-commit static/jquery.shtml       | tee static/index-hangzhou.html -a >/dev/null
config/sign-commit static/index_footer.shtml | tee static/index-hangzhou.html -a >/dev/null
config/sign-commit config/post-html/A06*     | tee static/index-hangzhou.html -a >/dev/null
echo Sign static/index-hangzhou.html
config/sign-commit DOCTYPE.shtml             | tee static/index-static.html    >/dev/null
config/sign-commit static/index_header.shtml | tee static/index-static.html -a >/dev/null
config/sign-commit header.shtml              | tee static/index-static.html -a >/dev/null
config/sign-commit static/index_body.shtml   | tee static/index-static.html -a >/dev/null
config/sign-commit footer.shtml              | tee static/index-static.html -a >/dev/null
