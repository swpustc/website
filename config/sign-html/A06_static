#!/bin/bash

### static
config/sign-commit DOCTYPE.shtml                 | tee static/400.html    >/dev/null
config/sign-commit static/400_header.shtml       | tee static/400.html -a >/dev/null
config/sign-commit header.shtml                  | tee static/400.html -a >/dev/null
config/sign-commit 400_body.shtml                | tee static/400.html -a >/dev/null
config/sign-commit footer.shtml                  | tee static/400.html -a >/dev/null
config/sign-commit static/jquery.shtml           | tee static/400.html -a >/dev/null
config/sign-commit static/errorpage_footer.shtml | tee static/400.html -a >/dev/null
config/sign-commit config/post-html/A06*         | tee static/400.html -a >/dev/null
echo Sign static/400.html
config/sign-commit DOCTYPE.shtml                 | tee static/403.html    >/dev/null
config/sign-commit static/403_header.shtml       | tee static/403.html -a >/dev/null
